{
  "name": "guardian-tabs",
  "version": "0.1.0",
  "dependencies": {
    "dayjs": "^1.8.19",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-scripts": "^3.3.0",
    "react-spinners": "^0.8.0",
    "react-spring": "^8.0.27",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "babel-loader": "^8.0.6",
    "customize-cra": "^0.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-import-resolver-babel-module": "^5.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "react-app-rewired": "^2.1.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "react-app",
      "airbnb",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "max-len": [
        "warn",
        {
          "code": 120,
          "ignoreStrings": true
        }
      ],
      "import/no-unresolved": [
        "error",
        {
          "caseSensitive": false
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/label-has-for": [
        2,
        {
          "components": [
            "Label"
          ],
          "required": {
            "every": [
              "id"
            ]
          }
        }
      ],
      "jsx-a11y/anchor-has-content": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "import/prefer-default-export": "off"
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "sitemap": "scripts/sitemap-builder.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tombrierley/guardian-tabs.git"
  },
  "author": "Tom Brierley",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/tombrierley/guardian-tabs/issues"
  },
  "engines": {
    "node": "12.x"
  }
}
